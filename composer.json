{
  "name": "m12u/twig-extensions-bundle",
  "description": "Twig extensions bundle",
  "type": "bundle",
  "autoload": {
    "psr-4": {
      "M12U\\Bundle\\TwigExtensionsBundle\\": "src/M12U/Bundle/TwigExtensionsBundle"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "M12U\\Bundle\\TwigExtensionsBundle\\Tests\\": "tests"
    },
    "classmap": [
      "tests/App/AppKernel.php"
    ]
  },
  "require": {
    "php": ">=5.5.9",
    "twig/extensions": "^1.5"
  },
  "require-dev": {
      "sensio/distribution-bundle": "^5.0.19",
      "sensio/framework-extra-bundle": "^5.0.0",
      "symfony/polyfill-apcu": "^1.0",
      "symfony/swiftmailer-bundle": "^2.6.4",
      "symfony/symfony": "3.4.*",
      "symfony/var-dumper": "^4.0"
  },
  "version": "1.0.0"
}
